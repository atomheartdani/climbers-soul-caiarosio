    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "stylelint": "^16.9.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.4.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts}": "eslint",
    "*.{js,ts}": "eslint --fix",
    "*": "prettier --ignore-unknown --write"
